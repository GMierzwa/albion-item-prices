{
    "source": "./src",
    "destination": "./docs",
    "includes": [
        "\\.js$"
    ],
    "plugins": [
        {
            "name": "./esdoc-plugins/proper-members.js"
        },
        {
            "name": "esdoc-node"
        },
        {
            "name": "esdoc-standard-plugin",
            "option": {
                "lint": {
                    "enable": true
                },
                "coverage": {
                    "enable": true
                },
                "accessor": {
                    "access": [
                        "public",
                        "protected",
                        "private"
                    ],
                    "autoPrivate": true
                },
                "undocumentIdentifier": {
                    "enable": true
                },
                "unexportedIdentifier": {
                    "enable": false
                },
                "typeInference": {
                    "enable": true
                },
                "test": {
                    "source": "./test/",
                    "interfaces": [
                        "describe",
                        "it",
                        "context",
                        "suite",
                        "test"
                    ],
                    "includes": [
                        "(spec|Spec|test|Test)\\.js$"
                    ],
                    "excludes": [
                        "\\.config\\.js$"
                    ]
                }
            }
        }
    ]
}